; This file assumes that fonts are stored at location 0x000.
; If you're using the interpreter that goes with it, you can
; specify font location at start with the '--font [location]'.

; You can compile with :
; python3 c8assembler.py examples/all_fonts.c8 -o all_flonts.c8c  
; Output file will be written in the dir where you launched the script.
#HEXDEF $
#MEMSTART $200

_start:
    ld v1, $0
    ld v2, $0
    ld I, $0
    drw v1, v2, $5